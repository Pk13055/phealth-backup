{% extends "healthprovider/dashboard/base.html.j2" %} 
{% load static %} 
{% block content %}
<div class="row bg-title">
	<div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
		<h4 class="page-title">Manage Clinician</h4>
	</div>
	<div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
		<ol class="breadcrumb">
			<li>
				<a href="index.html">Dashboard</a>
			</li>
			<li>
				<a href="#">Add Clinician</a>
			</li>
		</ol>
	</div>
	<!-- /.col-lg-12 -->
</div>
<div class="row white-box">
	<div class="col-md-12">
		<div class="panel panel-info">
			<div class="panel-heading">
				Add Clinician
			</div>
			<br>
			<div class="col-md-12">
			</div>
			<div class="col-md-12">
				<div class="panel-body">
					<div class="form-group">
						<label for="organizationname"> Full Name</label>
						<input type="text" class="form-control" id="name" placeholder="Full Name" name="name">
					</div>
					<div class="form-group">
						<label for="organizationname"> Mobile no.</label>
						<input type="number" class="form-control" id="number" placeholder="Clinic/Hospital/Diagnostics" name="number">
					</div>
					<div class="form-group">
						<label for="organizationname"> Email ID</label>
						<input type="email" class="form-control" id="emailid" placeholder="Clinic/Hospital/Diagnostics" name="emailid">
					</div>
					<!--<div class="checkbox">
						<input id="checkbox2" type="checkbox">
						<label for="checkbox2">By Signing Up I accept All Terms & Conditions</label>
					</div>-->
					<div class="form-group">
						<a href="#">
							<button class="btn btn-success pull-right">Add</button>
						</a>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
{% endblock content %}