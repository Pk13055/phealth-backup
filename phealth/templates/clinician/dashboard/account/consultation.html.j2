{% extends "clinician/dashboard/base.html.j2" %} {% load static %} {% block content %}
<div class="container-fluid">
	<div class="row bg-title">
		<div class="col-lg-3 col-md-4 col-sm-4 col-xs-12">
			<h4 class="page-title">Dashboard 1</h4>
		</div>
		<div class="col-lg-9 col-sm-8 col-md-8 col-xs-12">
			<ol class="breadcrumb">
				<li><a href="index.html">Dashboard</a></li>
				<li><a href="#">Basic Details</a></li>
			</ol>
		</div>
		<!-- /.col-lg-12 -->
	</div>
	<div class="row">
		<div class="col-md-12">
			<div class="panel panel-info">
				<div class="panel-heading">
					My Account
				</div>
				<div class="progress">
					<div class="progress-bar progress-bar-danger wow animated progress-animated" role="progressbar" aria-valuenow="60" aria-valuemin="0" aria-valuemax="100" style="width: 30%;"> <span class="sr-only">60% Complete</span> </div>
				</div>
				<div class="panel-wrapper collapse in" aria-expanded="true">
					<div class="panel-body">
						<form action="offerings.html" class="form-horizontal">
							<div class="form-body">
								<h3 class="box-title">Consultation Fee</h3>
								<hr class="m-t-0 m-b-40">
								<div class="row">
									<div class="col-md-6">
										<div class="form-group">
											<label for="consultationfee" class="col-md-3">Consultation Fee</label>
											<div class="col-md-9">
												<input type="text" class="form-control" name="consultationfee" id="consultationfee">
											</div>
										</div>
									</div>
									<div class="col-md-6">
										<div class="form-group">
											<label for="discount1st" class="col-md-3">Discount for 1st Consultation</label>
											<div class="col-md-9">
												<input type="text" class="form-control" name="discount1st" id="discount1st">
											</div>
										</div>
									</div>
									<div class="col-md-6">
										<div class="form-group">
											<label for="discountsubsequent" class="col-md-3">Discount for Subsequent Consultation</label>
											<div class="col-md-9">
												<input type="text" class="form-control" name="discountsubsequent" id="discountsubsequent">
											</div>
										</div>
									</div>
									<div class="col-md-6">
										<div class="form-group">
											<label for="activefrom" class="col-md-3">Active From</label>
											<div class="col-md-9">
												<input type="date" class="form-control" name="activefrom" id="activefrom">
											</div>
										</div>
									</div>
									<div class="col-md-6">
										<div class="form-group">
											<label for="activeto" class="col-md-3">Active To</label>
											<div class="col-md-9">
												<input type="date" class="form-control" name="activeto" id="activeto">
											</div>
										</div>
									</div>
									<div class="row">
										<hr>
									</div>
								</div>
							</div>
							<div class="form-actions">
								<div class="row">
									<div class="col-md-12">
										<div class="row">
											<div class="col-md-offset-10 col-md-2">
												<button type="submit" class="btn btn-success">Save & Go</button>
												<button type="button" class="btn btn-default">Cancel</button>
											</div>
										</div>
									</div>
									<div class="col-md-6"> </div>
								</div>
							</div>
						</form>
					</div>
				</div>
			</div>
		</div>
	</div>
</div>
<!-- /.container-fluid -->
{% endblock content %}
